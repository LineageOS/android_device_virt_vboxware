# Userdata (on separate disk)
/dev/block/sdb /data ext4 nodev,noatime,nosuid,discard latemount,check,quota,formattable
/dev/block/sdb /data f2fs nodev,noatime,nosuid,discard,reserve_root=32768 latemount,check,quota,formattable

# SD card (recovery only)
/dev/block/sdc /sdcard vfat ro,defaults recoveryonly
